{
    "name": "castaneras/laravel-fullcalendar",
    "description": "Laravel helper for FullCalendar.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Castanera",
            "email": "dan@castaneras.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "~9.0"
    },
    "autoload": {
        "psr-0": {
            "Castaneras\\LaravelFullcalendar": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Castaneras\\LaravelFullcalendar\\ServiceProvider"
            ],
            "aliases": {
                "Calendar": "Castaneras\\LaravelFullcalendar\\Facades\\Calendar"
            }
        }
    },
    "minimum-stability": "stable"
}
